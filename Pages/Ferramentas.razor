@page "/ferramentas"

<MudText Typo="Typo.h4">Ferramentas</MudText>
<br/>
<MudGrid>
    <MudItem Class="espaco" >
        <MudTooltip Text="Gerar número primo">
            <MudItem Class="carta">
                <MudIcon Class="icone" Icon="@Icons.Filled.AddChart"></MudIcon>
